{
    "name": "polidog/todo",
    "type": "project",
    "description": "",
    "license": "proprietary",
    "require": {
        "php": "~7.0",
        "roave/security-advisories": "dev-master",
        "bear/package": "~1.1",
        "josegonzalez/dotenv": "~2.0",
        "madapaja/twig-module": "^1.1",
        "ray/aura-sql-module": "~1.0",
        "ray/web-form-module": "^0.5.4",
        "koriym/now": "^1.0",
        "koriym/query-locator": "^1.3",
        "koriym/http-constants": "^1.0.1"
    },
    "require-dev": {
        "bear/qatools": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Polidog\\Todo\\": "src/"
        }
    },
    "scripts": {
        "setup": [
            "php bin/init_db.php"
        ],
        "test": [
            "phptest"
        ],
        "cs-fix": [
            "php-cs-fixer fix -v"
        ],
        "clean": "rm -rf var/tmp/* var/log/* tests/tmp/*",
        "serve": "php -S 127.0.0.1:8080 -t public",
        "api": "php bootstrap/api",
        "web": "php bootstrap/web"
    }
}
